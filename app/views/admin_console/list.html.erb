<h1>Listing Users</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Cell</th>
      <th>Other Phone</th>
      <th>CRM System</th>
      <th>Username</th>
      <th>Password</th>
      <th>Active Subscription</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.cell %></td>
        <td><%= user.phone2 %></td>
        <td><%= user.crm %></td>
        <td><%= user.un %></td>
        <td><%= user.pw %></td>
        <td><%= user.paid %></td>
     </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= button_to "Log Out", destroy_user_session_path,  :method=>'delete' %>